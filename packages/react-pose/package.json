{
  "name": "react-pose",
  "version": "2.1.0",
  "description": "A declarative animation library for React",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run measure",
    "watch": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "measure": "gzip -c dist/react-pose.js | wc -c",
    "prettier": "prettier ./src/* --write",
    "prepublishOnly": "npm run prettier && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popmotion/packages/react-pose"
  },
  "keywords": [
    "animation",
    "dom",
    "declarative",
    "popmotion",
    "react",
    "react animation"
  ],
  "author": "Matt Perry",
  "license": "MIT",
  "presets": [
    "env"
  ],
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "prettier": "1.11.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rollup": "^0.59.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "rollup-plugin-uglify": "^3.0.0",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "hey-listen": "^1.0.5",
    "popmotion-pose": "^2.0.1",
    "tslib": "^1.9.1"
  },
  "unpkg": "./dist/react-pose.js",
  "prettier": {
    "parser": "typescript",
    "singleQuote": true
  }
}
